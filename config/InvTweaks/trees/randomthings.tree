<!-- Note: 'stuff' matches everything, even items that don't appear here -->
<stuff treeVersion="1.12.2">
	<equipment treeOrder="10">
		<weapon treeOrder="10">
			<meleeWeapon treeOrder="10">
				<sword treeOrder="10">
					<sword class="ItemSword" data="{toolClass : &quot;&quot;}" mergeChildren="true" treeOrder="110">
						<swordDamage8 treeOrder="90">
							<itemSpectreSword id="randomthings:spectresword" treeOrder="20"/>
						</swordDamage8>
						<swordUnsorted treeOrder="170">
							<itemCreativeSword id="randomthings:creativesword" treeOrder="600"/>
						</swordUnsorted>
					</sword>
				</sword>
			</meleeWeapon>
		</weapon>
		<tool treeOrder="20">
			<keys treeOrder="250">
				<itemSpectreKey id="randomthings:spectrekey" treeOrder="60"/>
				<itemOpSpectreKey id="randomthings:opspectrekey" treeOrder="70"/>
			</keys>
		</tool>
		<armor treeOrder="30">
			<helmet class="ItemArmor" data="{ArmorType : &quot;head&quot;}" treeOrder="20">
				<helmet3 treeOrder="20">
					<itemSpectreHelmet id="randomthings:spectrehelmet" treeOrder="120"/>
				</helmet3>
			</helmet>
			<chestplate class="ItemArmor" data="{ArmorType:&quot;chest&quot;}" treeOrder="30">
				<chestplateArmor treeOrder="20">
					<chestplate9 treeOrder="20">
						<itemSpectreChestplate id="randomthings:spectrechestplate" treeOrder="10"/>
					</chestplate9>
				</chestplateArmor>
			</chestplate>
			<leggings class="ItemArmor" data="{ArmorType:&quot;legs&quot;}" treeOrder="40">
				<leggings7 treeOrder="20">
					<itemSpectreLeggings id="randomthings:spectreleggings" treeOrder="10"/>
				</leggings7>
			</leggings>
			<boots class="ItemArmor" data="{ArmorType:&quot;feet&quot;}" treeOrder="50">
				<boots3 treeOrder="30">
					<itemSpectreBoots id="randomthings:spectreboots" treeOrder="90"/>
				</boots3>
			</boots>
		</armor>
		<baubles treeOrder="40">
			<rings treeOrder="20">
				<itemObsidianSkullRing id="randomthings:obsidianskullring" treeOrder="370"/>
			</rings>
		</baubles>
	</equipment>
	<block treeOrder="20">
		<naturalBlock treeOrder="10">
			<dirts treeOrder="20">
				<fertilizedDirt treeOrder="110">
					<itemFertilizedDirt id="randomthings:fertilizeddirt" treeOrder="10"/>
				</fertilizedDirt>
			</dirts>
		</naturalBlock>
		<mechanism treeOrder="60">
			<lighting treeOrder="30">
				<torch treeOrder="10">
					<itemPeaceCandle id="randomthings:peacecandle" treeOrder="260"/>
				</torch>
			</lighting>
			<lever treeOrder="50">
				<itemWirelessLever id="randomthings:wirelesslever" damage="0" treeOrder="30"/>
			</lever>
			<itemAdvancedRedstoneTorch id="randomthings:advancedredstonetorch_on" treeOrder="130"/>
			<resourceTransfer treeOrder="320">
				<groundItemCollector treeOrder="10">
					<itemItemCollector id="randomthings:itemcollector" treeOrder="100"/>
					<itemAdvancedItemCollector id="randomthings:advanceditemcollector" treeOrder="110"/>
				</groundItemCollector>
			</resourceTransfer>
			<machines treeOrder="410">
				<decorationMachines treeOrder="190">
					<itemDyeingMachine id="randomthings:dyeingmachine" damage="0" treeOrder="50"/>
				</decorationMachines>
				<randomMachines treeOrder="220">
					<itemBiomePainter id="randomthings:biomepainter" treeOrder="80"/>
				</randomMachines>
			</machines>
		</mechanism>
	</block>
	<resources treeOrder="30">
		<miningResources treeOrder="10">
			<ingot treeOrder="130">
				<itemSpectreIngot id="randomthings:ingredient" damage="3" treeOrder="2000"/>
			</ingot>
		</miningResources>
		<huntingResources treeOrder="20">
			<string id="string" treeOrder="150">
				<itemSpectreString id="randomthings:ingredient" damage="12" treeOrder="70"/>
			</string>
		</huntingResources>
	</resources>
	<item treeOrder="40">
		<utility treeOrder="10">
			<flintAndSteel id="flint_and_steel" treeOrder="20">
				<itemBlazeandSteel id="randomthings:blazeandsteel" treeOrder="30"/>
			</flintAndSteel>
		</utility>
		<experienceStorage treeOrder="70">
			<itemExperienceImbue id="randomthings:imbue" treeOrder="60"/>
		</experienceStorage>
		<parts treeOrder="120">
			<tinkerParts treeOrder="20">
				<itemObsidianStick id="randomthings:ingredient" damage="1" treeOrder="720"/>
			</tinkerParts>
		</parts>
	</item>
</stuff>