<!-- Note: 'stuff' matches everything, even items that don't appear here -->
<stuff treeVersion="1.12.2">
	<equipment treeOrder="10">
		<tool treeOrder="20">
			<saws class="Item" data="{ToolClass : &quot;saw&quot;}" treeOrder="90">
				<itemDiamondBitSaw id="chiselsandbits:bitsaw_diamond" treeOrder="170"/>
			</saws>
			<wrench class="Item" data="{ToolClass : &quot;wrench&quot;}" mergeChildren="true" treeOrder="130">
				<itemWoodenWrench id="chiselsandbits:wrench_wood" treeOrder="290"/>
			</wrench>
			<chisel treeOrder="220">
				<itemDiamondChisel id="chiselsandbits:chisel_diamond" treeOrder="110"/>
				<itemGoldenChisel id="chiselsandbits:chisel_gold" treeOrder="120"/>
				<itemIronChisel id="chiselsandbits:chisel_iron" treeOrder="130"/>
				<itemStoneChisel id="chiselsandbits:chisel_stone" treeOrder="140"/>
			</chisel>
			<odometers treeOrder="300">
				<itemTapeMeasure id="chiselsandbits:tape_measure" treeOrder="20"/>
			</odometers>
		</tool>
	</equipment>
	<block treeOrder="20">
		<manufacturedBlock treeOrder="50">
			<constructionBlock treeOrder="70">
				<nook treeOrder="190">
					<itemChiseledBit id="chiselsandbits:block_bit" treeOrder="20"/>
				</nook>
			</constructionBlock>
		</manufacturedBlock>
	</block>
	<item treeOrder="40">
		<utility treeOrder="10">
			<storage treeOrder="30">
				<portableStorage treeOrder="100">
					<itemChiseledBitBag id="chiselsandbits:bit_bag" treeOrder="320"/>
				</portableStorage>
			</storage>
		</utility>
		<fluidStorage treeOrder="50">
			<fluidTanks treeOrder="30">
				<itemChiseledFluidBitTank id="chiselsandbits:bittank" treeOrder="20"/>
			</fluidTanks>
		</fluidStorage>
		<patterns treeOrder="100">
			<itemPositiveChiselDesign id="chiselsandbits:positiveprint" treeOrder="90"/>
			<itemNegativeChiselDesign id="chiselsandbits:negativeprint" treeOrder="100"/>
			<itemMirrorChiselDesign id="chiselsandbits:mirrorprint" treeOrder="110"/>
		</patterns>
	</item>
</stuff>