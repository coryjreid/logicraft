<!-- Note: 'stuff' matches everything, even items that don't appear here -->
<stuff treeVersion="1.12.2">
	<equipment treeOrder="10">
		<weapon treeOrder="10">
			<meleeWeapon treeOrder="10">
				<sword treeOrder="10">
					<sword class="ItemSword" data="{toolClass : &quot;&quot;}" mergeChildren="true" treeOrder="110">
						<swordDamage6 treeOrder="110">
							<itemBronzeSword id="techreborn:bronzesword" treeOrder="80"/>
							<itemPeridotSword id="techreborn:peridotsword" treeOrder="270"/>
							<itemRubySword id="techreborn:rubysword" treeOrder="280"/>
							<itemSapphireSword id="techreborn:sapphiresword" treeOrder="290"/>
						</swordDamage6>
					</sword>
				</sword>
			</meleeWeapon>
		</weapon>
		<tool treeOrder="20">
			<miningTool treeOrder="20">
				<pickaxe class="Item" data="{ToolClass : &quot;pickaxe&quot;}" mergeChildren="true" treeOrder="20">
					<DiamondPickLevel treeOrder="100">
						<itemBronzePickaxe id="techreborn:bronzepickaxe" treeOrder="80"/>
						<itemPeridotPickaxe id="techreborn:peridotpickaxe" treeOrder="300"/>
						<itemRubyPickaxe id="techreborn:rubypickaxe" treeOrder="390"/>
						<itemSapphirePickaxe id="techreborn:sapphirepickaxe" treeOrder="410"/>
					</DiamondPickLevel>
				</pickaxe>
			</miningTool>
			<diggingTool treeOrder="30">
				<shovel class="Item" data="{ToolClass : &quot;shovel&quot;}" mergeChildren="true" treeOrder="20">
					<itemBronzeShovel id="techreborn:bronzespade" treeOrder="110"/>
					<itemPeridotShovel id="techreborn:peridotspade" treeOrder="460"/>
					<itemRubyShovel id="techreborn:rubyspade" treeOrder="530"/>
					<itemSapphireShovel id="techreborn:sapphirespade" treeOrder="550"/>
				</shovel>
			</diggingTool>
			<choppingTool treeOrder="40">
				<axe class="Item" data="{ToolClass : &quot;axe&quot;}" mergeChildren="true" treeOrder="20">
					<itemBronzeAxe id="techreborn:bronzeaxe" treeOrder="180"/>
					<itemPeridotAxe id="techreborn:peridotaxe" treeOrder="520"/>
					<itemRubyAxe id="techreborn:rubyaxe" treeOrder="600"/>
					<itemSapphireAxe id="techreborn:sapphireaxe" treeOrder="620"/>
				</axe>
			</choppingTool>
			<farmingTool treeOrder="50">
				<hoe class="Item" data="{ToolClass : &quot;hoe&quot;}" mergeChildren="true" treeOrder="50">
					<itemBronzeHoe id="techreborn:bronzehoe" treeOrder="100"/>
					<itemPeridotHoe id="techreborn:peridothoe" treeOrder="440"/>
					<itemRubyHoe id="techreborn:rubyhoe" treeOrder="520"/>
					<itemSapphireHoe id="techreborn:sapphirehoe" treeOrder="540"/>
				</hoe>
			</farmingTool>
		</tool>
	</equipment>
	<item treeOrder="40">
		<fluidStorage treeOrder="50">
			<fluidCells treeOrder="60">
				<itemTRCell id="techreborn:dynamiccell" treeOrder="20"/>
			</fluidCells>
		</fluidStorage>
	</item>
</stuff>