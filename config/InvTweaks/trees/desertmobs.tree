<!-- Note: 'stuff' matches everything, even items that don't appear here -->
<stuff treeVersion="1.12.2">
	<equipment treeOrder="10">
		<weapon treeOrder="10">
			<munition treeOrder="20">
				<thrownMunitions treeOrder="80">
					<itemMudshotCharge id="desertmobs:mudshotcharge" treeOrder="120"/>
				</thrownMunitions>
				<scepters treeOrder="130">
					<itemMudshotScepter id="desertmobs:mudshotscepter" treeOrder="100"/>
				</scepters>
			</munition>
		</weapon>
		<tool treeOrder="20">
			<farmingTool treeOrder="50">
				<scythe class="Item" data="{ToolClass : &quot;scythe&quot;}" mergeChildren="true" treeOrder="10">
					<itemScytheScepter id="desertmobs:scythescepter" treeOrder="70"/>
					<itemThrowingScythe id="desertmobs:throwingscythe" treeOrder="90"/>
				</scythe>
			</farmingTool>
		</tool>
	</equipment>
	<item treeOrder="40">
		<utility treeOrder="10">
			<spawnerEggs treeOrder="220">
				<itemSpawnClink id="desertmobs:desertegg" damage="2" treeOrder="170"/>
				<itemSpawnCrusk id="desertmobs:desertegg" damage="1" treeOrder="180"/>
				<itemSpawnCryptZombie id="desertmobs:desertegg" damage="0" treeOrder="190"/>
				<itemSpawnErepede id="desertmobs:desertegg" damage="5" treeOrder="200"/>
				<itemSpawnGorgomite id="desertmobs:desertegg" damage="6" treeOrder="210"/>
				<itemSpawnJoust id="desertmobs:desertegg" damage="3" treeOrder="220"/>
				<itemSpawnJoustAlpha id="desertmobs:desertegg" damage="4" treeOrder="230"/>
				<itemSpawnManticore id="desertmobs:desertegg" damage="7" treeOrder="240"/>
			</spawnerEggs>
		</utility>
		<food treeOrder="160">
			<edibleFood treeOrder="10">
				<cookedFood treeOrder="50">
					<itemdesertmobs_joustmeatcooked id="desertmobs:joustmeatcooked" treeOrder="250"/>
				</cookedFood>
				<rawFood treeOrder="130">
					<rawMeat treeOrder="20">
						<itemdesertmobs_joustmeatraw id="desertmobs:joustmeatraw" treeOrder="210"/>
					</rawMeat>
				</rawFood>
			</edibleFood>
			<unedibleFood treeOrder="50">
				<cakes treeOrder="240">
					<itemdesertmobs_ambercake id="desertmobs:ambercake" treeOrder="140"/>
				</cakes>
			</unedibleFood>
			<hunting treeOrder="60">
				<taming treeOrder="30">
					<itemCruskTreat id="desertmobs:crusktreat" treeOrder="50"/>
					<itemErepedeTreat id="desertmobs:erepedetreat" treeOrder="60"/>
				</taming>
			</hunting>
		</food>
	</item>
</stuff>